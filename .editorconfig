# Source: https://github.com/airbnb/javascript/blob/master/.editorconfig
# Properties: https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties#current-universal-properties
root = true

[*]
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
end_of_line = lf
max_line_length = off
